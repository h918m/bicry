{
  "private": true,
  "name": "bicrypto",
  "version": "3.3.1",
  "type": "commonjs",
  "main": "./nuxt.config.ts",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "prisma": {
    "seed": "node ./prisma/seed/seed.mjs"
  },
  "scripts": {
    "dev": "nuxi dev .app",
    "dev:server": "nodemon",
    "start": "pm2 start production.config.js --env production",
    "start:nuxt": "node .app/.output/server/index.mjs",
    "start:server": "node ./dist/index.js",
    "stop": "pm2 stop all",
    "monit": "pm2 monit",
    "restart": "pm2 restart all",
    "plus": "pm2 plus",
    "build": "rimraf .app/.nuxt .app/.output && NODE_OPTIONS=--max-old-space-size=4096 nuxi build .app",
    "build:dev": "rimraf .app/.nuxt .app/.output && nuxi build .app",
    "build:server": "cd server && tsc && cd ..",
    "updator": "pnpm start && pnpm stop && pnpm update && pnpm build && pnpm merge && pnpm migrate:dev && pnpm seed &&  pnpm generate && pnpm build:server && pnpm start",
    "merge": "node ./scripts/merge.mjs",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "generate": "prisma generate",
    "seed": "prisma db seed",
    "kms:generate": "node ./scripts/kms/generate.mjs",
    "clean:all": "pnpm -r clean && rimraf .nuxt .output node_modules",
    "test": "lint:eslint && lint:prettier",
    "lint": "lint:eslint:fix && lint:prettier:fix",
    "lint:prettier": "prettier --check \"./**/*.(ts|vue|css|md)\"",
    "lint:prettier:fix": "prettier --write \"./**/*.(ts|vue|css|md)\"",
    "lint:eslint": "eslint -c .eslintrc.cjs --ext .vue,.ts .",
    "lint:eslint:fix": "eslint -c .eslintrc.cjs --fix --ext .vue,.ts ."
  },
